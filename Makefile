define get_journey
	sfclient --$(1) jc get --journey $(3) --category-id $(4) | gojq -cs '.[0].[0]' | gojq | tee $(1)/$(2)/$(3)/$(4).json
endef

define create_journey
	sfclient --$(1) jc create --journey $(3) --category-id $(4) --file ./$(1)/$(2)/$(3)/$(4).json
endef

prod.sg.search.dump:
	$(call get_journey,prod,sg,search,2561)
	$(call get_journey,prod,sg,search,5286)
	$(call get_journey,prod,sg,search,2698)
	$(call get_journey,prod,sg,search,5708)
	$(call get_journey,prod,sg,search,2033)
	$(call get_journey,prod,sg,search,2018)
	$(call get_journey,prod,sg,search,1701)
	$(call get_journey,prod,sg,search,1700)
	$(call get_journey,prod,sg,search,1702)

